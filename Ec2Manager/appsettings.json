{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "https_port": 443,
  "AllowedHosts": "*",
  "Ec2Manager": {
    "EnableReboot": false,
    "Accounts": [
      {
        "AccountName": "AccountName",
        "SecretName": "SecretNameHere",
        "AccessKey": "AccessKey",
        "Region": "Region",
        "NameTag": "NameTag",
        "TagToSearch": "TagToSearch",
        "SearchString": "RegExSearchString"
      }
    ]
  },
  "OidcAuth": {
    "Domain": "YourAuthDomainHere/oauth2/default",
    "ClientId": "ClientId",
    "ClientSecretKey": "ClientSecret",
    "ClientScopes": [
      "openid",
      "profile",
      "email",
      "AddYourScopesHere"
    ],
    "ClientAccountManagementClaim": "ClaimNameHere",
    "ClaimValueAccounts": [
      {
        "Value": "Group1",
        "EnableReboot": true,
        "Accounts": [
          "AccountsHere"
        ]
      },
      {
        "Value": "Group2",
        "EnableReboot": false,
        "Accounts": [
          "AccountsHere"
        ]
      }
    ]
  }
}