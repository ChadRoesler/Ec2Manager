{
  "https_port": "HttpsPortHere",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Ec2Manager": {
    "EnableReboot" :  false,
    "Accounts": [
      {
        "AccountName": "AccountName",
        "AccessKey": "HashedKey",
        "SecretKey": "HashedSecret",
        "Region": "Region",
        "NameTag": "TagForNameColumn",
        "TagToSearch": "TagToSearchOnForBaseResults",
        "SearchString": "RegExSearchString"
      }
    ]
  },
  "Okta": {
    "OktaDomain": "OktaDomainHere",
    "ClientId": "ClientId",
    "ClientSecret": "ClientSecret",
    "ClientScopes": [
      "ScopeName"
    ],
    "ClientAccountManagementClaim": "ClaimName",
    "ClaimValueAccounts": [
      {
        "Value": "ClaimVaue1",
        "EnableReboot" : false,
        "Accounts": [
          "AccountName"
        ]
      }
    ]
  }
